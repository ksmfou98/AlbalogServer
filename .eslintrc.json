{
  "extends": ["prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
          "endOfLine": "auto"
      }
  ],
    "no-unused-vars": "warn",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "no-loop-func": "off"
  }
}
